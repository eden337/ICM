            </Label>
         </children></HBox>
      <HBox fx:id="stsPane" prefHeight="300.0" prefWidth="200.0" />
      <HBox fx:id="freqPane" prefHeight="300.0" prefWidth="200.0" />
      <AnchorPane prefHeight="55.0" prefWidth="200.0">
         <children>
            <Button fx:id="generateReportBtn" layoutX="343.0" layoutY="15.0" mnemonicParsing="false" onAction="#BackToGenereteReport" text="Generate Report" />
            <Button fx:id="viewReportsBtn" layoutX="572.0" layoutY="15.0" mnemonicParsing="false" onAction="#backToViewReports" text="View Reports" />
         </children></AnchorPane>
   </children>
